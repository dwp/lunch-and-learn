{
  "name": "lunch-and-learn-site",
  "version": "0.1.0",
  "description": "A site, built using Middleman, to provide information on upcoming, and past, Lunch and Learn sessions.",
  "scripts": {
    "commitmsg": "commitlint --edit",
    "precommit": "lint-staged",
    "security-tests": "npm run security:outdated && npm run security:nsp",
    "security:nsp": "nsp check #",
    "security:outdated": "npm outdated || true",
    "security:snyk": "snyk test -dev",
    "test": "commitlint --from=develop --to=HEAD"
  },
  "devDependencies": {
    "@commitlint/cli": "5.2.8",
    "@dwp/commitlint-config-base": "1.0.0",
    "husky": "^0.14.3",
    "lint-staged": "6.1.1",
    "nsp": "^3.1.0",
    "snyk": "^1.64.0"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.itsshared.net:mike.shaw/lunch-and-learn-site.git"
  },
  "author": "Mike Shaw <mike.shaw@dwp.gsi.gov.uk>",
  "license": "ISC"
}
